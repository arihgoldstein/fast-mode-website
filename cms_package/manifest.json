  "pages": [
    {
      "path": "/",
      "file": "pages/index.html",
      "title": "Home",
      "editable": true
    },
    {
      "path": "/pricing",
      "file": "pages/pricing.html",
      "title": "Pricing",
      "editable": true
    },
    {
      "path": "/blog",
      "file": "pages/blog.html",
      "title": "Blog",
      "editable": true
    },
    {
      "path": "/privacy",
      "file": "pages/privacy.html",
      "title": "Privacy Policy",
      "editable": true
    },
    {
      "path": "/terms",
      "file": "pages/terms.html",
      "title": "Terms of Service",
      "editable": true
    }
  ],
  "templates": [
    {
      "name": "blog-post",
      "label": "Blog Post",
      "description": "Template for individual blog posts",
      "file": "templates/blog-post.html",
      "type": "blog",
      "basePath": "/blog"
    }
  ],
  "collections": {
    "blog": {
      "name": "Blog Posts",
      "template": "templates/blog-post.html",
      "basePath": "/blog",
      "fields": {
        "name": "title",
        "postSummary": "summary",
        "postBody": "content",
        "mainImage": "image",
        "author": "author",
        "publishedAt": "date",
        "featured": "boolean"
      }
    }
  },
  "cmsTemplates": {
    "blog": {
      "detailTemplate": "templates/blog-post.html",
      "basePath": "/blog"
    }
  },
  "defaultHeadHtml": "<link rel=\"preconnect\" href=\"https://fonts.googleapis.com\"><link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin><link href=\"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&display=swap\" rel=\"stylesheet\"><link rel=\"stylesheet\" href=\"/public/css/style.css\">"
